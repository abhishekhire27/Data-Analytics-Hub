<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<AnchorPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <VBox prefHeight="376.0" prefWidth="539.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="User Name : ">
               <VBox.margin>
                  <Insets top="150.0" />
               </VBox.margin>
            </Text>
            <TextField fx:id="userName">
               <VBox.margin>
                  <Insets left="150.0" right="80.0" />
               </VBox.margin>
            </TextField>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password :" />
            <TextField fx:id="password">
               <VBox.margin>
                  <Insets left="150.0" right="80.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginHandler" text="Login" />
            <Button fx:id="registerButton" mnemonicParsing="false" onAction="#switchToRegisterSceneHandler" text="Register" />
         </children>
      </VBox>
   </children>
</AnchorPane>
